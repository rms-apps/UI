{
  "name": "@rms-apps/ui",
  "private": true,
  "scripts": {
    "remove-dist": "rm -rf ./packages/*/dist/ ./packages/*/.turbo ./.turbo",
    "clean": "rm -rf ./packages/*/dist/ ./packages/*/.turbo ./packages/*/node_modules node_modules .expo .turbo",
    "lint-css": "stylelint \"**/*.scss\" --allow-empty-input",
    "check-types": "turbo run check-types",
    "lint": "turbo run lint",
    "check-all": "pnpm run check-types && pnpm run lint",
    "prepare": "is-ci || husky install",
    "prebuild": "pnpm run remove-dist",
    "build": "turbo run build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "release": "pnpm run build && changeset publish",
    "clean-install": "pnpm run clean && pnpm install && pnpm run check-all && pnpm run build",
    "dev": "cd apps/expo-native && pnpm start",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "create-component": "node scripts/scaffolds/component-starter-code.mjs",
    "lint-pkg": "node scripts/dev/lint-pkg.js",
    "build-pkg": "node scripts/dev/build-pkg.js"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.7",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/js": "9.37.0",
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "changesets": "^1.0.2",
    "eslint": "9.37.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "eslint-plugin-react-native": "5.0.0",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "nativewind": "^4.1.23",
    "prettier": "^3.6.2",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-config-tailwindcss": "1.0.0",
    "tailwindcss": "^3.4.17",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "add": "2.0.6",
    "expo": "^54.0.13",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-linear-gradient": "15.0.7",
    "expo-linking": "~8.0.8",
    "expo-notifications": "0.32.12",
    "expo-router": "~6.0.12",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "pnpm": "10.18.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-css-interop": "0.2.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-mobile-ads": "15.8.1",
    "react-native-modal": "14.0.0-rc.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.14.0",
    "react-native-svg-transformer": "1.5.1",
    "react-native-toast-message": "2.3.3",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  }
}
